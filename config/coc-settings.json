{
  "diagnostic.errorSign": "\uf467",
  "diagnostic.hintSign": "\uf864",
  "diagnostic.infoSign": "\uf129",
  "diagnostic.warningSign": "\uf071",
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"],
      "rootPatterns": [
        ".git/",
        ".gitignore",
        "CMakeLists.txt",
        ".ccls",
        "compile_commands.json"
      ],
      "initializationOptions": {
        "highlight": {
          "lsRanges": true
        }
      }
    }
  },
  "markdownlint.config": {
    "default": true,
    "line_length": false
  },
  "rust-analyzer.server.path": "rust-analyzer",
  "snippets.snipmate.enable": false,
  "suggest.completionItemKindLabels": {
    "class": "\ufd2f",
    "color": "\ue22b",
    "constant": "\uf8ff",
    "default": "\uf872",
    "enum": "\ufbdf",
    "enumMember": "\uf7d7",
    "field": "\uf02b",
    "operator": "\uf695",
    "reference": "\uf064",
    "event": "\uf09e",
    "file": "\uf15b",
    "folder": "\uf07b",
    "function": "\uf0ad",
    "interface": "\uf12e",
    "keyword": "\uf805",
    "method": "\uf6ed",
    "module": "\uf1b2",
    "property": "\uf02b",
    "snippet": "\uf22f",
    "struct": "\uf085",
    "text": "\ue612",
    "typeParameter": "\ufa35",
    "unit": "\uf06c",
    "value": "\uf8a0",
    "variable": "\uf013"
  }
}
